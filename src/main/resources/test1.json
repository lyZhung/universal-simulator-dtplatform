{
  "version":["1","2","3"],
  "disable_existing_loggers": "true|false",
  "formatters": {
    "standard": {
      "format": "[a-zA-Z0-9]{5}\\.[0-9]{5}\\.[0-9]{3}"
    },
    "logstash": {
      "()": "logstash_formatter\\.LogstashFormatter"
    },
    "elogging_format": {
      "format": "[a-zA-Z0-9]{3}\\.[0-9]{3}\\.[0-9]{3}"
    }
  },
  "handlers": {
    "console": {
      "class": "logging\\.StreamHandler",
      "level": "DEBUG",
      "formatter": "elogging_format"
    },
    "info_file_handler": {
      "class": "logging\\.handlers\\.RotatingFileHandler",
      "level": "ERROR|WARN|INFO|DEBUG",
      "formatter": "elogging_format",
      "filename": "/var/log/applog/info\\.log"
    },
    "debug_file_handler": {
      "class": "logging\\.handlers\\.RotatingFileHandler",
      "level": "ERROR|WARN|INFO|DEBUG",
      "formatter": "elogging_format",
      "filename": "/var/log/applog/debug\\.log"
    },
    "error_file_handler": {
      "class": "logging\\.handlers\\.RotatingFileHandler",
      "level": "ERROR|WARN|INFO|DEBUG",
      "formatter": "elogging_format",
      "filename": "/var/log/applog/errors|corrects\\.log"
    },
    "kafka_handler":{
      "class":"python_kafka_logging\\.KafkaHandler\\.KafkaLoggingHandler",
      "level": "ERROR|WARN|INFO|DEBUG",
      "formatter":"logstash|flume",
      "hosts_list":"192\\.168\\.33\\.21:6667",
      "topic":"dtyunxi_elog"
    }
  }
  "loggers": {
    "elog": {
      "level": "ERROR|WARN|INFO|DEBUG",
      "handlers": ["console","debug_file_handler","kafka_handler"],
      "propagate": "no"
    },
    "root": {
      "level": "ERROR|WARN|INFO|DEBUG",
      "handlers": ["console","debug_file_handler","kafka_handler"]
    }
  },
  "path": [
    {
      "event": "bookedTrip"
    },
    {
      "event": "bookedTripStart"
    },
    {
      "event": "bookedTripEnd"
    }
  ]
}